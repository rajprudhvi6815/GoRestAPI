	"item": [
		{
			"name": "Create User",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var random=Math.random().toString(36).substring(2);\r",
							"var usermail=\"raj\"+random+\"@yopmail.com\";\r",
							"var username=\"raj\"+random;\r",
							"pm.collectionVariables.set(\"gomail\",usermail);\r",
							"pm.collectionVariables.set(\"gouname\",username);\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Verify status is 201\", () => {\r",
							"    pm.response.to.have.status(201)\r",
							"})\r",
							"\r",
							"var jsonData = pm.response.json();\r",
							"pm.collectionVariables.set(\"gid\", jsonData.id);\r",
							"\r",
							"\r",
							"\r",
							"// Retrieve the saved \"gid\" value from the collection variable\r",
							"pm.test(\"Verify collection variable is created\", function () {\r",
							"    var savedGid = pm.collectionVariables.get(\"gid\");\r",
							"\r",
							"    pm.expect(savedGid).to.exist;\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
							"key": "token",
							"value": "{{Tok}}",
							"type": "string"
						}
					]
				},
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"name\": \"{{gouname}}\",\r\n    \"gender\": \"male\",\r\n    \"email\": \"{{gomail}}\",\r\n    \"status\": \"active\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "https://gorest.co.in/public/v2/users",
					"protocol": "https",
					"host": [
						"gorest",
						"co",
						"in"
					],
					"path": [
						"public",
						"v2",
						"users"
					]
				}
			},
			"response": []
		},
		{
			"name": "Get User Details",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"id in response is same as collection variable\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.id).to.eql(pm.collectionVariables.get(\"gid\"));\r",
							"});\r",
							"\r",
							"pm.test(\"name in response is same as collection variable\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.name).to.eql(pm.collectionVariables.get(\"gouname\"));\r",
							"});\r",
							"\r",
							"pm.test(\"email in response is same as collection variable\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.email).to.eql(pm.variables.get(\"gomail\"));\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
